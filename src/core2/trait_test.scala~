object Core {

  class Sort

  object Real extends Sort

  abstract class Term[T <: Sort]

  trait binary[L <: Sort, R <: Sort, T <: Sort] extends Term[T] {
    val left  : Term[L]
    val right : Term[L]
  }

  class plus(val left : Real, val right : Real) extends Term[Real] with binary[Real, Real, Real]



}
